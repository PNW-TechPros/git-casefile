{
  "name": "natural-lenses",
  "version": "2.0.0",
  "description": "A JavaScript-native lens (and ancillary optics) system loosely based on Kmett-style lenses.",
  "type": "commonjs",
  "main": "./index",
  "module": "index.mjs",
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.js"
    },
    "./*": {
      "import": "./*.mjs",
      "require": "./*.js"
    }
  },
  "sideEffects": [
    "./src/stdlib_support/*.js",
    "./src/immutable.js"
  ],
  "imports": {
    "#this": {
      "import": "./index.mjs",
      "default": "./index.js"
    },
    "#this/*": {
      "import": "./*.mjs",
      "default": "./*.js"
    }
  },
  "scripts": {
    "build:cjs": "cross-env BABEL_ENV=cjs babel src -d cjs --source-maps --compact --minified --no-comments",
    "build:esm": "cross-env BABEL_ENV=esm babel src -d esm",
    "build": "npm-run-all -l clean -p build:cjs build:esm",
    "clean": "rm -f {cjs,esm}/{,**}*.js",
    "console": "node --experimental-repl-await --async-stack-traces",
    "prepare": "npm run build",
    "coverage": "nyc --reporter=lcov --reporter=text-summary npm run test",
    "make:doc": "jsdoc -c jsdoc.conf.js -r -P package.json -R README.md -d doc",
    "prepublishOnly": "node scripts/docs/build-new.js",
    "postpublish": "node scripts/docs/publish.js",
    "pretest": "cross-env NODE_ENV=test npm run prepare",
    "test": "mocha",
    "predtest": "npm run prepare",
    "dtest": "mocha --inspect-brk test"
  },
  "keywords": [
    "lens",
    "lenses",
    "optics",
    "kmett"
  ],
  "author": "Richard T. Weeks",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.17.3",
    "@babel/core": "^7.17.5",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@istanbuljs/nyc-config-babel": "^3.0.0",
    "babel-plugin-istanbul": "^6.1.1",
    "chai": "^4.3.4",
    "cross-env": "^7.0.3",
    "docdash": "^1.2.0",
    "ejs": "^3.1.6",
    "fs-extra": "^10.0.1",
    "immutable": "^3.8.2",
    "jsdoc": "^3.6.10",
    "mocha": "^9.1.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "semver-parser": "^4.0.0",
    "sinon": "^13.0.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.17.2",
    "underscore": "^1.13.1"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-babel"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PNW-TechPros/js-natural-lenses.git"
  },
  "bugs": {
    "url": "https://github.com/PNW-TechPros/js-natural-lenses/issues"
  },
  "homepage": "https://github.com/PNW-TechPros/js-natural-lenses#readme"
}
